{
  "name": "eslintrc",
  "version": "1.0.0",
  "description": "A collection of my common eslint configurations",
  "main": ".eslintrc.json",
  "scripts": {
    "lint": "find . -path ./node_modules -prune -o -type f -name '*.json' -print | xargs -I {} jsonlint -q {}",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakesidsmith/eslintrc.git"
  },
  "keywords": [
    "eslint",
    "config",
    "es5"
  ],
  "author": "Jake 'Sid' Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakesidsmith/eslintrc/issues"
  },
  "homepage": "https://github.com/jakesidsmith/eslintrc#readme",
  "dependencies": {
    "babel-eslint": "7.1.1",
    "eslint": "3.15.0",
    "eslint-plugin-react": "6.10.0",
    "eslint-plugin-react-native": "2.2.1"
  },
  "devDependencies": {
    "jsonlint": "1.6.2"
  }
}
