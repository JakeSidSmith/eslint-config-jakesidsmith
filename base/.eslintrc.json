{
  "parserOptions": {
    "ecmaVersion": 5
  },
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1,
        "outerIIFEBody": 1,
        "MemberExpression": 1,
        "FunctionDeclaration": {"body": 1, "parameters": 1},
        "FunctionExpression": {"body": 1, "parameters": 1},
        "CallExpression": {"arguments": 1},
        "ArrayExpression": 1,
        "ObjectExpression": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1
      }
    ],
    "max-len": [
      2,
      120,
      2,
      {
        "ignoreComments": true
      }
    ],
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": false
      }
    ],
    "dot-location": [
      2,
      "property"
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "block-spacing": [
      2,
      "never"
    ],
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "yoda": [
      2,
      "never"
    ],
    "space-before-function-paren": [
      2,
      "always"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "keyword-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "spaced-comment": [
      2,
      "always"
    ],
    "wrap-iife": [
      2,
      "inside"
    ],
    "no-duplicate-imports": [
      2,
      {
        "includeExports": true
      }
    ],
    "space-unary-ops": 2,
    "space-infix-ops": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-irregular-whitespace": 2,
    "no-multi-spaces": 2,
    "no-whitespace-before-property": 2,
    "no-regex-spaces": 2,
    "no-lone-blocks": 2,
    "eqeqeq": 2,
    "block-scoped-var": 2,
    "no-constant-condition": 2,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-else-return": 2,
    "no-eval": 2,
    "no-multi-str": 2,
    "no-self-compare": 2,
    "no-useless-call": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-use-before-define": 2,
    "radix": 2,
    "curly": 2,
    "no-fallthrough": 2,
    "default-case": 2,
    "no-plusplus": 2,
    "dot-notation": 2,
    "eol-last": 2,
    "no-spaced-func": 2,
    "no-func-assign": 2,
    "new-parens": 2,
    "no-extra-semi": 2
  }
}
